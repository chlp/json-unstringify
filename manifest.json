{
  "manifest_version": 3,
  "name": "JSON Unstringify",
  "homepage_url": "https://github.com/chlp/json-unstringify",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "description": "Beautifies selected nested JSON by expanding it into readable objects"
}
